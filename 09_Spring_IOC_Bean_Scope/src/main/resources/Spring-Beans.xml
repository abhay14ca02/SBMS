<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="hp" class="com.rajabhay.beans.HPPrinter"/>
	<bean id="sp" class="com.rajabhay.beans.SonyPrinter"/>
	<!--By default scope is sigleton -->
	<bean id="atm" class="com.rajabhay.beans.ATM" scope="prototype">
		<!--<property name="printer" ref="hp" />-->
		<property name="printer" ref="sp" />
	</bean>
</beans>